{
  "name": "modern-web-app",
  "version": "1.0.0",
  "description": "A modern web application using React, Apollo GraphQL, TypeORM, Postgres, and Express.js.",
  "repository": "https://github.com/meebix/modern-web-app",
  "author": "Michael Donahue <mtdonahue1@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "NODE_ENV=production node .build/server/index.js",
    "dev": "nodemon",
    "lint:ts": "eslint src --ext '.ts,.tsx' --quiet",
    "lint:css": "stylelint src/**/*.css --quiet",
    "lint": "yarn lint:ts && yarn lint:css",
    "prettier:fix": "prettier \"**/*.{ts,tsx}\" --write",
    "ts": "tsc & tsc -p server/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migration:create": "typeorm migration:create -n untitled",
    "db:migration:run": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:run",
    "db:migration:revert": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:show": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:show",
    "db:seed:create": "typeorm migration:create -n untitled -d server/seeds",
    "db:seed:run": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:run -c seed",
    "db:seed:reset": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:revert -c seed"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "antd": "^3.26.11",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.10.1",
    "argon2": "^0.26.0",
    "assign-deep": "^1.0.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^2.9.0",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "glob": "^7.1.6",
    "googleapis": "^47.0.0",
    "graphql": "^14.6.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.13",
    "graphql-tag": "^2.10.3",
    "helmet": "^3.21.2",
    "is-glob": "^4.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.11.1",
    "next": "^9.2.2",
    "pg": "^7.18.2",
    "pino": "^5.16.0",
    "pino-pretty": "^3.6.0",
    "prisma": "^1.34.0",
    "prisma-client-lib": "^1.34.0",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "stripe": "^8.29.0",
    "tailwindcss": "^1.2.0",
    "typeorm": "^0.2.22",
    "universal-cookie": "^4.0.3",
    "uuid": "^3.4.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@next/bundle-analyzer": "^9.2.2",
    "@types/chance": "^1.0.9",
    "@types/cookie-parser": "1.4.2",
    "@types/csurf": "1.9.36",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.3",
    "@types/jsonwebtoken": "8.3.7",
    "@types/node": "13.7.4",
    "@types/pino": "^5.15.5",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "@types/uuid": "3.4.7",
    "@types/webpack-env": "1.15.1",
    "@typescript-eslint/eslint-plugin": "2.20.0",

{
  "name": "modern-web-app",
  "version": "1.0.0",
  "description": "A modern web application using React, Apollo GraphQL, Prisma DB, and Express.js",
  "repository": "https://github.com/meebix/modern-web-app",
  "author": "Michael Donahue <mtdonahue1@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "start": "NODE_ENV=production node .build/server/index.js",
    "dev": "nodemon",
    "lint": "eslint pages --ext '.ts,.tsx' --quiet",
    "prettier:fix": "prettier \"**/*.{ts,tsx}\" --write",
    "ts": "tsc & tsc -p server/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migration:create": "typeorm migration:create -n Untitled",
    "db:migration:run": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:run",
    "db:migration:revert": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:show": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:show",
    "db:seed:create": "typeorm migration:create -n Untitled -d server/seeds",
    "db:seed:run": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:run -c seed",
    "db:seed:reset": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:revert -c seed"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.25.3",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.9.12",
    "argon2": "^0.24.0",
    "assign-deep": "^1.0.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "date-fns": "^1.30.1",
    "deepmerge": "^3.3.0",
    "express": "^4.17.1",
    "formik": "^1.5.7",
    "glob": "^7.1.6",
    "googleapis": "^40.0.0",
    "graphql": "^14.5.8",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.7.1",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.2",
    "is-glob": "^4.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.10.3",
    "next": "^9.1.4",
    "pg": "^7.14.0",
    "pino": "^5.14.0",
    "pino-pretty": "^3.4.0",
    "prisma": "^1.34.0",
    "prisma-client-lib": "^1.34.0",
    "react": "^16.12.0",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "stripe": "^7.14.0",
    "tailwindcss": "^1.1.4",
    "typeorm": "^0.2.20",
    "universal-cookie": "^4.0.2",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/cookie-parser": "1.4.2",
    "@types/csurf": "1.9.36",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/helmet": "0.0.45",
    "@types/jest": "24.0.23",
    "@types/jsonwebtoken": "8.3.5",
    "@types/next": "9.0.0",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/uuid": "3.4.6",
    "@types/webpack-env": "1.14.1",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.3-alpha.13",
    "@zeit/next-css": "1.0.1",
    "autoprefixer": "9.7.2",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-module-resolver": "3.2.0",
    "chance": "1.1.3",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.7.1",
    "eslint-config-airbnb-typescript": "4.0.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "file-loader": "4.0.0",
    "husky": "2.4.1",
    "jest": "24.9.0",
    "lint-staged": "8.2.1",
    "nodemon": "1.19.1",
    "null-loader": "3.0.0",
    "postcss": "7.0.23",
    "postcss-custom-properties": "9.0.2",
    "postcss-functions": "3.0.0",
    "postcss-import": "12.0.1",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "5.0.2",
    "prettier": "1.19.1",
    "react-dev-utils": "9.1.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.2",
    "url-loader": "2.0.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "concurrent": true,
    "linters": {
      "*.{ts,tsx}": [
        "yarn prettier:fix",
        "yarn lint --fix",
        "git add"
      ]
    }
  }
}

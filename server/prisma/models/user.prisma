type User {
  id: ID! @id @unique
  role: Role!
  userAccount: UserAccount! @relation(link: INLINE)
  customerAccount: Customer @relation(link: INLINE)
  firstName: String
  lastName: String
  email: String! @unique
  password: String
  phoneCountryCode: String
  phone: String
  country: String
  address1: String
  address2: String
  city: String
  state: String
  postalCode: String
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
  deletedAt: DateTime
}
